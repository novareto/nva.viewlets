<div id="document_actions" class="container" tal:define="normalizeString nocall: context/@@plone/normalizeString;">
  <ul>
    <li tal:condition="view/webcode">
      <span class="action_icon"></span>Webcode: <span tal:replace="viewlet/getWebcode"/>
    </li>
    <li tal:repeat="action view/actions"
	tal:attributes="id python:'document-action-' + normalizeString(action['id'])">
      <a href="" tal:attributes="href action/url; title action/description" tal:content="daction/title"/>
    </li>
  </ul>
</div>
